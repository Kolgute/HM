[32m+[m[32mÔªøusing System;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing System.IO;[m
[32m+[m[32musing System.IO.Compression;[m
[32m+[m[32musing System.Linq;[m
[32m+[m[32musing System.Text;[m
[32m+[m[32musing System.Threading.Tasks;[m
[32m+[m
[32m+[m[32mnamespace EPAM_HM_COMPRESS[m
[32m+[m[32m{[m
[32m+[m[32m    class Program[m
[32m+[m[32m    {[m
[32m+[m[32m        static void Main(string[] args)[m
[32m+[m[32m        {[m
[32m+[m[32m            string sourceFile = "E://HM/EPAM_HM_Compress/goncharov_ivan-obyknovennaja_istorija.txt"; // –∏—Å—Ö–æ–¥–Ω—ã–π —Ñ–∞–π–ª[m
[32m+[m[32m            string compressedFile = "E://HM/EPAM_HM_Compress/goncharov_ivan-obyknovennaja_istorija.gz"; // —Å–∂–∞—Ç—ã–π —Ñ–∞–π–ª[m
[32m+[m[32m            string targetFile = "E://HM/EPAM_HM_Compress/goncharov_new.txt"; // –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–Ω—ã–π —Ñ–∞–π–ª[m
[32m+[m
[32m+[m[32m            Compress(sourceFile, compressedFile);[m
[32m+[m
[32m+[m[32m            Decompress(compressedFile, targetFile);[m
[32m+[m
[32m+[m[32m            Console.ReadLine();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public static void Compress(string sourceFile, string compressedFile)[m
[32m+[m[32m        {[m
[32m+[m[32m            // –ø–æ—Ç–æ–∫ –¥–ª—è —á—Ç–µ–Ω–∏—è –∏—Å—Ö–æ–¥–Ω–æ–≥–æ —Ñ–∞–π–ª–∞[m
[32m+[m[32m            using (FileStream sourceStream = new FileStream(sourceFile, FileMode.OpenOrCreate))[m
[32m+[m[32m            {[m
[32m+[m[32m                // –ø–æ—Ç–æ–∫ –¥–ª—è –∑–∞–ø–∏—Å–∏ —Å–∂–∞—Ç–æ–≥–æ —Ñ–∞–π–ª–∞[m
[32m+[m[32m                using (FileStream targetStream = File.Create(compressedFile))[m
[32m+[m[32m                {[m
[32m+[m[32m                    // –ø–æ—Ç–æ–∫ –∞—Ä—Ö–∏–≤–∞—Ü–∏–∏[m
[32m+[m[32m                    using (GZipStream compressionStream = new GZipStream(targetStream, CompressionMode.Compress))[m
[32m+[m[32m                    {[m
[32m+[m[32m                        sourceStream.CopyTo(compressionStream); // –∫–æ–ø–∏—Ä—É–µ–º –±–∞–π—Ç—ã –∏–∑ –æ–¥–Ω–æ–≥–æ –ø–æ—Ç–æ–∫–∞ –≤ –¥—Ä—É–≥–æ–π[m
[32m+[m[32m                        Console.WriteLine($"–°–∂–∞—Ç–∏–µ —Ñ–∞–π–ª–∞ {sourceFile} –∑–∞–≤–µ—Ä—à–µ–Ω–æ. \n–ò—Å—Ö–æ–¥–Ω—ã–π —Ä–∞–∑–º–µ—Ä: {sourceStream.Length.ToString()}  \n—Å–∂–∞—Ç—ã–π —Ä–∞–∑–º–µ—Ä: {targetStream.Length.ToString()}.");[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public static void Decompress(string compressedFile, string targetFile)[m
[32m+[m[32m        {[m
[32m+[m[32m            // –ø–æ—Ç–æ–∫ –¥–ª—è —á—Ç–µ–Ω–∏—è –∏–∑ —Å–∂–∞—Ç–æ–≥–æ —Ñ–∞–π–ª–∞[m
[32m+[m[32m            using (FileStream sourceStream = new FileStream(compressedFile, FileMode.OpenOrCreate))[m
[32m+[m[32m            {[m
[32m+[m[32m                // –ø–æ—Ç–æ–∫ –¥–ª—è –∑–∞–ø–∏—Å–∏ –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–Ω–æ–≥–æ —Ñ–∞–π–ª–∞[m
[32m+[m[32m                using (FileStream targetStream = File.Create(targetFile))[m
[32m+[m[32m                {[m
[32m+[m[32m                    // –ø–æ—Ç–æ–∫ —Ä–∞–∑–∞—Ä—Ö–∏–≤–∞—Ü–∏–∏[m
[32m+[m[32m                    using (GZipStream decompressionStream = new GZipStream(sourceStream, CompressionMode.Decompress))[m
[32m+[m[32m                    {[m
[32m+[m[32m                        decompressionStream.CopyTo(targetStream);[m
[32m+[m[32m                        Console.WriteLine($"–í–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω —Ñ–∞–π–ª: {targetFile}");[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
